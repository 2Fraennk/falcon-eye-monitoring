SHELL := /bin/bash

prepare-fem-demoapp:
	@mkdir -p target/demoapp
	@cp -rn demoapp target/

start: prepare-fem-demoapp
	docker-compose --file target/demoapp/docker-compose-fem-demoapp.yml up -d

stop: prepare-fem-demoapp
	docker-compose --file target/demoapp/docker-compose-fem-demoapp.yml down

clean:
	@rm -rf target/demoapp
